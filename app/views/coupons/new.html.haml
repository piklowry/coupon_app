%h4
	= form_for @new_coupon, url: coupons_path do |f|
		%p
			= f.label :title
			= f.text_field :title
			%br
			= f.label :description
			= f.text_field :description
			%br
			= f.label :sale_offer
			= f.text_field :sale_offer
			%br
			= f.label :company_name
			= f.text_field :company_name
			%br
			= f.file_field :coupon_pic
			- f.hidden_field :coupon_pic
			%br
			= f.submit