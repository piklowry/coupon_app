- @coupons.each do |d|
  .col-sm-4.col-xs-6
    .panel.panel-default
      .panel-thumbnail
        %img.img-responsive{:height => "100%",:width => "100%",:overflow => "hidden"}
          = image_tag d.coupon_pic.url
      .panel-body
        %p.lead 
          = d.title
        %p 
          = d.description
        %p
          = d.sale_offer
          = link_to "Download Photo", download_coupon_pic_coupon_path(d)
      


